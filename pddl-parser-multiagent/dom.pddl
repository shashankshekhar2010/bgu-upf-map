( DEFINE ( DOMAIN MAZE )
( :REQUIREMENTS :NEGATIVE-PRECONDITIONS :TYPING :FACTORED-PRIVACY )
( :TYPES
	AGENT - OBJECT
	BOAT - OBJECT
	LOCATION - OBJECT
	DOOR - OBJECT
	BRIDGE - OBJECT
	SWITCH - OBJECT
	AGENT-COUNT - OBJECT
)
( :CONSTANTS
	ACOUNT-0 - AGENT-COUNT
)
( :PREDICATES
	( AT ?AGENT0 - AGENT ?LOCATION1 - LOCATION )
	( AT ?BOAT0 - BOAT ?LOCATION1 - LOCATION )
	( HAS-SWITCH ?SWITCH0 - SWITCH ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION ?LOCATION3 - LOCATION )
	( BLOCKED ?LOCATION0 - LOCATION ?LOCATION1 - LOCATION )
	( HAS-DOOR ?DOOR0 - DOOR ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( HAS-BOAT ?BOAT0 - BOAT ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( HAS-BRIDGE ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( POS-HAS-BRIDGE ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( NEG-HAS-BRIDGE ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( AFREE )
	( ACTIVITY-FREE )
	( ATEMP )
	( TAKEN ?AGENT0 - AGENT )
	( CONSEC ?AGENT-COUNT0 - AGENT-COUNT ?AGENT-COUNT1 - AGENT-COUNT )
	( CONSEC1 ?AGENT-COUNT0 - AGENT-COUNT ?AGENT-COUNT1 - AGENT-COUNT )
	( CONSEC1 ?AGENT0 - AGENT )
	( ACTIVE-V3 ?BRIDGE0 - BRIDGE )
	( COUNT-V3 ?AGENT-COUNT0 - AGENT-COUNT )
	( SAT-V3 ?AGENT-COUNT0 - AGENT-COUNT )
)
( :ACTION START-V3
  :PARAMETERS ( ?BRIDGE0 - BRIDGE )
  :PRECONDITION
	( AND
		( AFREE )
	)
  :EFFECT
	( AND
		( NOT ( AFREE ) )
		( ACTIVE-V3 ?BRIDGE0 )
		( COUNT-V3 ACOUNT-0 )
	)
)
( :ACTION DO-CROSS
  :PARAMETERS ( ?AGENT0 - AGENT ?BRIDGE1 - BRIDGE ?LOCATION2 - LOCATION ?LOCATION3 - LOCATION ?AGENT-COUNT4 - AGENT-COUNT ?AGENT-COUNT5 - AGENT-COUNT )
  :PRECONDITION
	( AND
		( AT ?AGENT0 ?LOCATION2 )
		( HAS-BRIDGE ?BRIDGE1 ?LOCATION2 ?LOCATION3 )
		( ACTIVE-V3 ?BRIDGE1 )
		( NOT ( TAKEN ?AGENT0 ) )
		( COUNT-V3 ?AGENT-COUNT4 )
		( CONSEC ?AGENT-COUNT4 ?AGENT-COUNT5 )
	)
  :EFFECT
	( AND
		( AT ?AGENT0 ?LOCATION3 )
		( NOT ( AT ?AGENT0 ?LOCATION2 ) )
		( NEG-HAS-BRIDGE ?BRIDGE1 ?LOCATION2 ?LOCATION3 )
		( POS-HAS-BRIDGE ?BRIDGE1 ?LOCATION3 ?LOCATION2 )
		( TAKEN ?AGENT0 )
		( NOT ( COUNT-V3 ?AGENT-COUNT4 ) )
		( COUNT-V3 ?AGENT-COUNT5 )
	)
)
( :ACTION END-V3
  :PARAMETERS ( ?BRIDGE0 - BRIDGE ?AGENT-COUNT1 - AGENT-COUNT )
  :PRECONDITION
	( AND
		( ACTIVITY-FREE )
		( COUNT-V3 ?AGENT-COUNT1 )
		( SAT-V3 ?AGENT-COUNT1 )
		( ACTIVE-V3 ?BRIDGE0 )
	)
  :EFFECT
	( AND
		( NOT ( COUNT-V3 ?AGENT-COUNT1 ) )
		( ATEMP )
		( NOT ( ACTIVE-V3 ?BRIDGE0 ) )
		( FORALL
			( ?AGENT2 - AGENT )
			( NOT ( TAKEN ?AGENT2 ) )
		)
	)
)
( :ACTION ADD-HAS-BRIDGE
  :PARAMETERS ( ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
  :PRECONDITION
	( AND
		( ATEMP )
		( POS-HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 )
	)
  :EFFECT
	( AND
		( HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 )
		( NOT ( POS-HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 ) )
	)
)
( :ACTION DELETE-HAS-BRIDGE
  :PARAMETERS ( ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
  :PRECONDITION
	( AND
		( ATEMP )
		( NEG-HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 )
	)
  :EFFECT
	( AND
		( NOT ( HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 ) )
		( NOT ( NEG-HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 ) )
	)
)
( :ACTION POS-NEG-FREE
  :PARAMETERS ( )
  :PRECONDITION
	( AND
		( ATEMP )
		( FORALL
			( ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
			( AND
				( NOT ( POS-HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 ) )
				( NOT ( NEG-HAS-BRIDGE ?BRIDGE0 ?LOCATION1 ?LOCATION2 ) )
			)
		)
	)
  :EFFECT
	( AND
		( AFREE )
		( NOT ( ATEMP ) )
	)
)
)
