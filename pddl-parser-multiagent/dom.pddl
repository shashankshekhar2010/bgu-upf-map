( DEFINE ( DOMAIN MAZE )
( :REQUIREMENTS :TYPING )
( :TYPES
	AGENT - OBJECT
	BOAT - OBJECT
	LOCATION - OBJECT
	DOOR - OBJECT
	BRIDGE - OBJECT
	SWITCH - OBJECT
	AGENT-COUNT - OBJECT
)
( :CONSTANTS
	ACOUNT-0 - AGENT-COUNT
)
( :PREDICATES
	( AT ?AGENT0 - AGENT ?LOCATION1 - LOCATION )
	( HAS-SWITCH ?SWITCH0 - SWITCH ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION ?LOCATION3 - LOCATION )
	( BLOCKED ?LOCATION0 - LOCATION ?LOCATION1 - LOCATION )
	( HAS-DOOR ?DOOR0 - DOOR ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( HAS-BOAT ?BOAT0 - BOAT ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( POS-HAS-BOAT ?BOAT0 - BOAT ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( NEG-HAS-BOAT ?BOAT0 - BOAT ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( HAS-BRIDGE ?BRIDGE0 - BRIDGE ?LOCATION1 - LOCATION ?LOCATION2 - LOCATION )
	( NEXT ?AGENT0 - AGENT )
	( ATBOAT ?BOAT0 - BOAT ?LOCATION1 - LOCATION )
	( TAKEN ?AGENT0 - AGENT )
	( CONSEC ?AGENT-COUNT0 - AGENT-COUNT ?AGENT-COUNT1 - AGENT-COUNT )
	( ACTIVE-V1 ?BOAT0 - BOAT )
	( COUNT-V1 ?AGENT-COUNT0 - AGENT-COUNT )
	( SAT-V1 ?AGENT-COUNT0 - AGENT-COUNT )
	( AFREE )
	( IN-JOINT )
	( NEXT-ACTIVITY-SAIL-2 )
)
( :ACTION START-V1
  :PARAMETERS ( ?BOAT0 - BOAT )
  :PRECONDITION
	( AND
		( AFREE )
	)
  :EFFECT
	( AND
		( NOT ( AFREE ) )
		( ACTIVE-V1 ?BOAT0 )
		( COUNT-V1 ACOUNT-0 )
	)
)
( :ACTION ACTIVITY-SAIL-1
  :PARAMETERS ( ?AGENT0 - AGENT ?BOAT2 - BOAT ?LOCATION3 - LOCATION ?LOCATION4 - LOCATION ?AGENT-COUNT5 - AGENT-COUNT ?AGENT-COUNT6 - AGENT-COUNT )
  :PRECONDITION
	( AND
		( AT ?AGENT0 ?LOCATION3 )
		( ATBOAT ?BOAT2 ?LOCATION3 )
		( NOT ( IN-JOINT ) )
		( ACTIVE-V1 ?LOCATION4 )
		( NOT ( TAKEN ?AGENT0 ) )
		( COUNT-V1 ?AGENT-COUNT5 )
		( CONSEC ?AGENT-COUNT5 ?AGENT-COUNT6 )
	)
  :EFFECT
	( AND
		( AT ?AGENT0 ?LOCATION4 )
		( NOT ( AT ?AGENT0 ?LOCATION3 ) )
		( NOT ( ATBOAT ?BOAT2 ?LOCATION3 ) )
		( ATBOAT ?BOAT2 ?LOCATION4 )
		( TAKEN ?AGENT0 )
		( NOT ( COUNT-V1 ?AGENT-COUNT5 ) )
		( COUNT-V1 ?AGENT-COUNT6 )
		( IN-JOINT )
		( NEXT-ACTIVITY-SAIL-2 )
	)
)
( :ACTION ACTIVITY-SAIL-2
  :PARAMETERS ( ?AGENT0 - AGENT ?BOAT2 - BOAT ?LOCATION3 - LOCATION ?LOCATION4 - LOCATION ?AGENT-COUNT5 - AGENT-COUNT ?AGENT-COUNT6 - AGENT-COUNT )
  :PRECONDITION
	( AND
		( AT ?AGENT0 ?LOCATION3 )
		( ATBOAT ?BOAT2 ?LOCATION3 )
		( NEXT-ACTIVITY-SAIL-2 )
		( ACTIVE-V1 ?LOCATION4 )
		( NOT ( TAKEN ?AGENT0 ) )
		( COUNT-V1 ?AGENT-COUNT5 )
		( CONSEC ?AGENT-COUNT5 ?AGENT-COUNT6 )
	)
  :EFFECT
	( AND
		( AT ?AGENT0 ?LOCATION4 )
		( NOT ( AT ?AGENT0 ?LOCATION3 ) )
		( NOT ( ATBOAT ?BOAT2 ?LOCATION3 ) )
		( ATBOAT ?BOAT2 ?LOCATION4 )
		( NOT ( IN-JOINT ) )
		( NOT ( NEXT-ACTIVITY-SAIL-2 ) )
		( TAKEN ?AGENT0 )
		( NOT ( COUNT-V1 ?AGENT-COUNT5 ) )
		( COUNT-V1 ?AGENT-COUNT6 )
	)
)
( :ACTION DO-ROW
  :PARAMETERS ( ?AGENT0 - AGENT ?BOAT1 - BOAT ?LOCATION2 - LOCATION ?LOCATION3 - LOCATION ?AGENT-COUNT4 - AGENT-COUNT ?AGENT-COUNT5 - AGENT-COUNT )
  :PRECONDITION
	( AND
		( AT ?AGENT0 ?LOCATION2 )
		( HAS-BOAT ?BOAT1 ?LOCATION2 ?LOCATION3 )
		( NOT ( IN-JOINT ) )
		( ACTIVE-V1 ?BOAT1 )
		( NOT ( TAKEN ?AGENT0 ) )
		( COUNT-V1 ?AGENT-COUNT4 )
		( CONSEC ?AGENT-COUNT4 ?AGENT-COUNT5 )
	)
  :EFFECT
	( AND
		( AT ?AGENT0 ?LOCATION3 )
		( NOT ( AT ?AGENT0 ?LOCATION2 ) )
		( NEG-HAS-BOAT ?BOAT1 ?LOCATION2 ?LOCATION3 )
		( POS-HAS-BOAT ?BOAT1 ?LOCATION3 ?LOCATION2 )
		( TAKEN ?AGENT0 )
		( NOT ( COUNT-V1 ?AGENT-COUNT4 ) )
		( COUNT-V1 ?AGENT-COUNT5 )
	)
)
( :ACTION END-V1
  :PARAMETERS ( ?BOAT0 - BOAT ?AGENT-COUNT1 - AGENT-COUNT )
  :PRECONDITION
	( AND
		( COUNT-V1 ?AGENT-COUNT1 )
		( SAT-V1 ?AGENT-COUNT1 )
		( ACTIVE-V1 ?BOAT0 )
	)
  :EFFECT
	( AND
		( NOT ( COUNT-V1 ?AGENT-COUNT1 ) )
		( AFREE )
		( NOT ( ACTIVE-V1 ?BOAT0 ) )
		( FORALL
			( ?AGENT2 - AGENT )
			( NOT ( TAKEN ?AGENT2 ) )
		)
		( FORALL
			( ?LOCATION2 - LOCATION ?LOCATION3 - LOCATION )
			( AND
				( WHEN
					( POS-HAS-BOAT ?BOAT0 ?LOCATION2 ?LOCATION3 )
					( AND
						( HAS-BOAT ?BOAT0 ?LOCATION2 ?LOCATION3 )
						( NOT ( POS-HAS-BOAT ?BOAT0 ?LOCATION2 ?LOCATION3 ) )
					)
				)
				( WHEN
					( NEG-HAS-BOAT ?BOAT0 ?LOCATION2 ?LOCATION3 )
					( AND
						( NOT ( HAS-BOAT ?BOAT0 ?LOCATION2 ?LOCATION3 ) )
						( NOT ( NEG-HAS-BOAT ?BOAT0 ?LOCATION2 ?LOCATION3 ) )
					)
				)
			)
		)
	)
)
)
